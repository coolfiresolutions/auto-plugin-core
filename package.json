    "changelog",
    "core",
    "coolfire",
    "coolfire solutions"
  ],
  "author": "Coolfire Solutions <support@coolfiresolutions.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coolfiresolutions/auto-plugin-core/issues"
  },
  "homepage": "https://github.com/coolfiresolutions/auto-plugin-core#readme",
  "engines": {
    "node": ">=16.6.0",
    "yarn": ">=3.0.0",
