{
  "extends": ["config:base", ":pinOnlyDevDependencies", ":semanticCommits"],
  "onboarding": false,
  "requireConfig": false,
  "timezone": "America/Chicago",
  "branchPrefix": "renovate-custom/",
  "gitAuthor": "CFS Devops <support@coolfiresolutions.com>",
  "automergeType": "branch",
  "labels": ["dependencies"],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    },
    {
      "matchUpdateTypes": ["pin", "patch", "digest", "lockFileMaintenance"],
      "automerge": true
    }
  ]
}
